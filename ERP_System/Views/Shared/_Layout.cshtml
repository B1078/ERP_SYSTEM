<script type="text/javascript" src="~/assets/js/script.js "></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.17.5/xlsx.full.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/PapaParse/5.3.0/papaparse.min.js"></script>
<!-- Datatable  js -->
<script src="~/assets/dtcdn/jquery.datatables.min.js"></script>

<script>
$(document).ready(function(){
			
			$(document).keydown(function (event) {
				if (event.ctrlKey && event.which === 82) {
		         	event.preventDefault();
				   	$("#Form_reset_btn").click();
				} else if (event.ctrlKey && event.which === 83) {
				  	event.preventDefault();
					$("#Form_Save_btn").click();
				} else if (event.ctrlKey && event.which === 68) {
					event.preventDefault();
					$("#Form_Draft_btn").click();
				} else if (event.ctrlKey && event.which === 65) {
					event.preventDefault();
					$("#AddCpd").click();
				} 
			});
	   $('select').css({ width: "100%", backgroundColor: "white" }).select2();
			$("#Form_reset_btn").click(function () {
				$('select').next('.select2-container').removeClass("select2-error");
				// Iterate through each select element
				$('select').each(function () {
					// Check if the first option has a value of ""
					if ($(this).find('option:first').val() === "") {
						// If the first option's value is empty, set the select's value to empty and trigger change
						$(this).val("").trigger('change');
					} else {
						// Otherwise, set the first option as selected
						$(this).val($(this).find('option:first').val()).trigger('change');
					}
				});
			});
			// Hide the loader initially
			$("#loader").hide();

			// Toggle loader on button click
			$("#toggleBtn").click(function () {
				$("#loader").toggle();
			});
		

	   //Scrol Behaviour
	   {
		 $('a[href^="#"]').on('click', function (event) {
		 	var target = $($(this).attr('href'));
		 
		 	if (target.length) {
		 		event.preventDefault();
		 		$('html, body').animate({
		 			scrollTop: target.offset().top
		 		}, 800); // You can adjust the speed here (e.g., change 800 to your desired speed in milliseconds)
		 	}
		 });
	   }
	   //Set Title
	   {

				$(".Title").text($(".webtitle").text())
	   }
	   // Modal Functionality
	   {

			// Close modal
			$(".closeModal").click(function () {
				$(".modal").fadeOut();
			});
			// Close modal if clicked outside of it
			$(window).click(function (event) {
				if (event.target == $(".modal")[0]) {
					$(".modal").fadeOut();
				}
			});
	        // Responsive behavior
	        $(window).on('resize', function () {
	        	var modalContent = $('.modal-content');
	        	if ($(window).width() <= 768) {
	        		modalContent.css('max-width', '90%');
	        	} else {
	        		modalContent.css('max-width', '80%');
	        	}
	        });
	   }
       // Checkbox Logic	   
		$(".toggleSwitch").click(function () {
			$(this).val(this.checked ? "Y" : "N")
		});
	    $(document).on("click", ".tbltoggleSwitch",function () {
		    $(this).val(this.checked ? "Y" : "N")
		})	
	    //Add Memu Active class
		{
		    var url = window.location.href;
			var parts = url.split('/');
			var lastWord = parts.pop();
			$(".DefalutActive").removeClass('active')
			$("." + lastWord).removeClass('active')
			$("." + lastWord).addClass('active')
			$("." + lastWord).parents('.pcoded-hasmenu').addClass('active');
			$("." + lastWord).parents('.pcoded-hasmenu').addClass('pcoded-trigger');
			$(".pcoded-hasmenu").click(function(){
				$(".DefalutActive").removeClass('active')
				$(".pcoded-hasmenu").removeClass('active')
				var td = $(this);
				$(this).addClass('active')
			})
			$(".DefalutActive").click(function () {
				$(".pcoded-hasmenu").removeClass('active')
				$(".DefalutActive").removeClass('active')
				$(this).addClass('active')
			})
			// Remove Valid Warning From Inputs
			$('input,textarea').on('input', function () {
				$(this).removeClass("border-danger");
			});

			$('select').change(function () {
				$(this).removeClass("border-danger");
			});
		}
		//INPUT Validations
		{
				$(".PANINPUT").on("input", function () {
					var This = $(this);
					var PanNO = $(this).val();
					PanNO = PanNO.substr(0, 10);
					$(this).val(PanNO);
					validatePan(PanNO,"Input",This);
				});

				$(".PANINPUT").on("change", function () {
					var This = $(this);
					var PanNO = $(this).val();
					validatePan(PanNO, "Change", This);
				});

				function validatePan(PanNO, Event, This) {
					if (PanNO.length === 10 && Event == "Input") {
						var panPattern = /^[A-Z]{5}[0-9]{4}[A-Z]{1}$/;
						if (!panPattern.test(PanNO)) {
							This.addClass("border-danger");
							notify("Enter Valid PAN Number..!", "", "top", "right", "fa fa-warning mr-2", "danger", "animated fadeInRight", "animated fadeOutRight");
							return false;
						} else {
							This.removeClass("border-danger");
							return true;
						}
					}
					if (Event == "Change") {
						var panPattern = /^[A-Z]{5}[0-9]{4}[A-Z]{1}$/;
						if (!panPattern.test(PanNO)) {
							This.addClass("border-danger");
							notify("Enter Valid PAN Number..!", "", "top", "right", "fa fa-warning mr-2", "danger", "animated fadeInRight", "animated fadeOutRight");
							return false;
						} else {
							This.removeClass("border-danger");
							return true;
						}
					}
				}
			  	$(document).on('input', '.Discountinp', function () {
					var discount = parseFloat($(this).val());
					if (isNaN(discount)) {
						$(this).val(0.00);
					} else if (discount >= 100) {
						$(this).val(100);
					} else {
						$(this).val(discount);
					}
				});
			$(".MAILINPUT").on("change", function () {
				if (!/^([a-z])+([a-zA-Z0-9_.+-])+\@@(([a-z])+\.+?(com|co|in|org|net|edu|info|gov|vekomy))\.?(com|co|in|org|net|edu|info|gov)?$/.test($(this).val())) {
					$(this).addClass("border-danger");
					notify("Enter Valid Email Address..!", "", "top", "right", "fa fa-warning mr-2", "danger", "animated fadeInRight", "animated fadeOutRight")
					return false;
				   }else{
					return true;
				}
			});
			$(".MONOINPUT").on("input", function () {
				var PanNO = $(this).val();
				PanNO = PanNO.substr(0, 10);
				$(this).val(PanNO);
			});
			$(".PHONENOINPUT").on("input", function () {
				var PanNO = $(this).val();
				PanNO = PanNO.substr(0, 12);
				$(this).val(PanNO);
			});
			$(".GSTININPUT").on("input", function () {
				var GSTIN = $(this).val();
					var This = $(this);

				GSTIN = GSTIN.substr(0, 15);
				$(this).val(GSTIN);
					validateGSTIN(GSTIN, "Input", This);
			});
			$(".GSTININPUT").on("Change", function () {
				var GSTIN = $(this).val();
				var This = $(this);

				GSTIN = GSTIN.substr(0, 15);
				$(this).val(GSTIN);
				validateGSTIN(GSTIN, "Change", This);
			});

			$(".SWIFTINPUT").on("input", function () {
				var GSTIN = $(this).val();
				GSTIN = GSTIN.substr(0, 11);
				$(this).val(GSTIN);
			});
			$(".IFSCINPUT").on("input", function () {
					var This = $(this);
				var GSTIN = $(this).val();
				GSTIN = GSTIN.substr(0, 11);
				$(this).val(GSTIN);
			});
			$(".ACCOUNTNOINPUT").on("change", function () {
				var GSTIN = $(this).val();
					GSTIN = GSTIN.substr(0, 15);
					$(this).val(GSTIN);
			});
				function validateGSTIN(GSTIN, Event, This) {
				if (GSTIN.length === 15 && Event == "Input") {
					var gstinPattern = /^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Za-z]{1}[Z]{1}[0-9A-Za-z]{1}$/;
					if (!gstinPattern.test(GSTIN)) {
							This.addClass("border-danger");
						notify("Enter Valid GSTIN Number..!", "", "top", "right", "fa fa-warning mr-2", "danger", "animated fadeInRight", "animated fadeOutRight");
						return false;
					} else {
							This.removeClass("border-danger");
						return true;
					}
				}
			    if(Event == "Change") {
				   var gstinPattern = /^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Za-z]{1}[Z]{1}[0-9A-Za-z]{1}$/;
					if (!gstinPattern.test(GSTIN)) {
							This.addClass("border-danger");
						notify("Enter Valid GSTIN Number..!", "", "top", "right", "fa fa-warning mr-2", "danger", "animated fadeInRight", "animated fadeOutRight");
						return false;
					} else {
							This.removeClass("border-danger");
						return true;
					}
				}
			}
		

		}
		//Region Destroy Select2
		{
				// Destroy Select2
				$('#DocType').select2('destroy');
				// Remove any additional classes or attributes added by Select2
				$('#DocType').removeClass('select2-hidden-accessible');
				// Optionally, you may also remove any event listeners or data attached by Select2
				$('#DocType').off('.select2').removeData('select2');
		}
			
})

		function MaxLengthInp(This, Maxlength) {
			var value = $(This).val();
			value = value.substr(0, Maxlength);
			$(This).val(value);
		}
		function notify(message,Title, from, align, icon, type, animIn, animOut) {
				$.growl({
					icon: icon,
					title: Title,
					message: message,
					url: ''
				}, {
					element: 'body',
					type: type,
					allow_dismiss: true,
					placement: {
						from: from,
						align: align
					},
					offset: {
						x: 30,
						y: 30
					},
					spacing: 10,
					z_index: 999999,
					delay: 2500,
					timer: 1000,
					url_target: '_blank',
					mouse_over: false,
					animate: {
						enter: animIn,
						exit: animOut
					},
					icon_type: 'class',
					template: '<div data-growl="container" class="alert" role="alert">' +
						'<button type="button" class="close" data-growl="dismiss">' +
						'<span aria-hidden="true">&times;</span>' +
						'<span class="sr-only">Close</span>' +
						'</button>' +
						'<span data-growl="icon"></span>' +
						'<span data-growl="title"></span>' +
						'<span data-growl="message"></span>' +
						'<a href="#" data-growl="url"></a>' +
						'</div>'
				});
			};
			</script>
</body>

</html>
