                    var data = Copyfromheaderdata.find(obj => obj.SalsDelId === idValue);
                    if (data) {
                        BindHeaderCopyFrom(data);
                        LoadFreightCopy(idValue, "SD");
                    }
                    $.ajax({
                        url: '@Url.Action("GETITEMDET", "Delivery")',
                        type: 'GET',
                        data: { ID: idValue, Flag: "O" },
                        dataType: 'json',
                        success: function (response) {
                            response.forEach(function (item) {
                                item.BaseDocLineNum = item.LineNum;
                                item.BaseDocEntry = data.DocNum;
                                item.BaseDocType = 'SD';
                                item.SalesRReqDetId = "";
                                item.SalesRReqId = "";
                                item.RowStatus = 'O';
                                item.Qty = item.OpenQty;
                                item.Remarks = "";
                            });
                            ApplyDetailstable(response, "Table1");
                            $(".Qty").trigger('input');
                            $('#CopyFromModal').modal('hide');
                        },
                        error: function (error) {
                            notify(error.responseText, "", "top", "right", "fa fa-warning mr-2", "danger", "animated fadeInRight", "animated fadeOutRight");
                            console.error('Error:', error.responseText);
                        }
                    });
                }
                else if (rowData && rowData.BlKAId) {
                    var idValue = rowData.BlKAId;
                    var data = Copyfromheaderdata.find(obj => obj.BlKAId === idValue);
                    if (data) {
                        BindHeaderCopyFrom(data);
                    }
                    $.ajax({
                        url: '@Url.Action("GETDETAILS", "PurchaseBlanketAgreement")',
                        type: 'GET',
                        data: { Id: idValue },
                        dataType: 'json',
                        success: function (response) {
                            var BlKAId = '';
                            var i = 1;
                            response.forEach(function (item) {
                                item.LineNum = i;
                                item.GdReceiptDetId = '';
                                item.GdReceiptId = '';
                                item.Price = item.UnitPrice || '0';
                                item.Qty = item.PlanQty || '0';
                                item.DisPer = '0';
                                item.TaxAmountLC = '0';
                                item.LineTotal = '0';
                                item.CalcTotalLC = '0';
                                item.LocName = '';
                                item.RowStatus = 'O';
                                item.Qty = item.OpenQty;
                                item.ItemHSNNo = '';
                                item.BarCode = '';
                                item.BaseDocLineNum = item.LineNum;
                                item.BaseDocEntry = idValue;
                                item.BaseDocType = 'SBA';
                                //  item.QtyInWhs = '';
                                //  item.Weight = '';
                                item.Remarks = item.ItemRemark;
                                item.GLAcctName = '';
                                i++;
                            });
                            ApplyDetailstable(response, "Table1");
                            $(".Qty").trigger('input');
                            $('#CopyFromModal').modal('hide');
                        },
                        error: function (error) {
                            notify(error.responseText, "", "top", "right", "fa fa-warning mr-2", "danger", "animated fadeInRight", "animated fadeOutRight");
                            console.error('Error:', error.responseText);
                        }
                    });
                }
            }

            // Function to bind data to header fields from the selected copy data
            function BindHeaderCopyFrom(data) {
                Object.entries(data).forEach(([key, value]) => {
                    var input = $("#" + key);
                    if (!["Tax", "TotBefDis", "TotPayDue", "Status", "DisPer", "DisAmt"].includes(key)) {
                        if (input.hasClass("select2-hidden-accessible")) {
                            input.val(value).trigger("change");
                        } else {
                            if (["DocumentDt", "PostingDt", "RequiredDt"].includes(key)) {
                                value = formatAndSetDate(value);
                            }
                            input.val(value);
                        }
                    }
                });
            }
            function LoadFreightCopy(ID, Flag) {
                var Url = "";
                var obj = "";
                if (Flag == "PR") {
                    Url = '@Url.Action("GETFREIGHT", "PurchaseRequest")';
                    obj = { PurReqId: ID }
                } else if (Flag == "SQ") {
                    Url = '@Url.Action("GETFREIGHT", "SalesQuotation")';
                    obj = { Id: ID }
                }
                else if (Flag == "SD") {
                    Url = '@Url.Action("GETFREIGHT", "Delivery")';
                    obj = { Id: ID }
                }
                $.ajax({
                    url: Url,
                    type: 'GET',
                    data: obj,
                    dataType: 'json',
                    success: function (response) {
                        for (var i = 0; i < response.length; i++) {
                            response[i].SalsOFretId = "";
                        }
                        if (response.length > 0) {
                            LoadFreighttbl('', response)
                        } else {
                            LoadDefFreight()
                        }
                    },
                    error: function (error) {
                        notify(error.responseText, "", "top", "right", "fa fa-warning mr-2", "danger", "animated fadeInRight", "animated fadeOutRight");
                        console.error('Error:', error.responseText);
                    }
                });
            }
        }

        // Region Navigation
        {
            var $table = $("#Table");
            $table.find("tbody tr:first").addClass("active");
            $(".btn_forward").click(function () {
                var $currentRow = $table.find(".active");
                var $nextRow = $currentRow.next("tr");
                if ($nextRow.length > 0) {
                    $currentRow.removeClass("active");
                    $nextRow.addClass("active");
                    BindHederRecords($nextRow, "N");
                } else {
                    var $firstRow = $table.find("tbody tr:first");
                    $table.find(".active").removeClass("active");
                    $firstRow.addClass("active");
                    BindHederRecords($firstRow, "N");

                }
            });
            $(".btn_backward").click(function () {
                var $currentRow = $table.find(".active");
                var $prevRow = $currentRow.prev("tr");
                if ($prevRow.length > 0) {
                    $currentRow.removeClass("active");
                    $prevRow.addClass("active");
                    BindHederRecords($prevRow);
                } else {
                    var $lastRow = $table.find("tr:last");
                    $table.find(".active").removeClass("active");
                    $lastRow.addClass("active");
                    BindHederRecords($lastRow, "N");
                }
            });
            $(".btn_first").click(function () {
                var $firstRow = $table.find("tbody tr:first");
                $table.find(".active").removeClass("active");
                $firstRow.addClass("active");
                BindHederRecords($firstRow, "N");
            });
            $(".btn_last").click(function () {
                var $lastRow = $table.find("tr:last");
                $table.find(".active").removeClass("active");
                $lastRow.addClass("active");
                BindHederRecords($lastRow, "N");
            });
            $(".btn_reload").click(function () {
                ResetForm()
                $("#Form_reset_btn").click();
                HideBtn()
            });
        }
    });
</script>

