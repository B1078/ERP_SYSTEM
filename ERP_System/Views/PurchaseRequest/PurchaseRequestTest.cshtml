        function calculateLineTotal(row) {
            if (row) {
                var priceText = row.find('.Price').val().trim();
                var quantityText = row.find('.Qty').val().trim();

                console.log("Price Text:", priceText);
                console.log("Quantity Text:", quantityText);

                // If price or quantity text is empty, set to '0' to avoid errors
                var price = parseFloat(priceText) || 0;
                var quantity = parseInt(quantityText) || 0;

                console.log("Parsed Price:", price);
                console.log("Parsed Quantity:", quantity);

                // Calculate only if quantity and price are greater than 0
                if (price > 0 && quantity > 0) {
                    var lineTotal = (price * quantity).toFixed(2);
                    row.find('.Tbl_LineTotal').text(lineTotal);
                } else {
