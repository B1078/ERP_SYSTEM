                        'PostingDt': { title: 'Posting Date', className: 'tbl_PostingDt' },
                        'Requester': { title: 'Request Type', className: 'tbl_Requester' },
                        'Reqtname': { title: 'Requester', className: 'tbl_Reqtname' },
                        'DeliveryDt': { title: 'Delivery Date', className: 'tbl_DeliveryDate' },
                        'DocumentDt': { title: 'Document Date', className: 'tbl_DocumentDt' },
                        'RequiredDt': { title: 'Required Date', className: 'tbl_RequiredDt' },
                        'Remarks': { title: 'Remarks', className: 'tbl_Remarks' }
                    };
                } else if (Value === 'GDR') {
                    columnNames = {
                        'GdReceiptId': { title: 'Sales Quotation No', className: 'Ctbl_SalsQuotId d-none' },
                        'DocNum': { title: 'Sales Quotation No', className: 'Ctbl_DocNum' },
                        'PostingDt': { title: 'Posting Date', className: 'Ctbl_PostingDt' },
                        'DocumentDt': { title: 'Document Date', className: 'Ctbl_DocumentDt' },
                        'Remarks': { title: 'Remarks', className: 'Ctbl_Remarks' }
                    };
                }

                Object.keys(columnNames).forEach(function (key) {
                    columns.push({
                        data: key,
                        title: columnNames[key].title,
                        className: columnNames[key].className + ' text-center' // Assign class dynamically
                    });
                });

                $('#' + Table_Id).DataTable({
                    data: jsonData,
                    columns: columns,
                    scrollX: true,
                    responsive: true,
                    scrollY: '600px',
                    ordering: true,
                    autoWidth: true,
                    orderCellsTop: true,
                    scrollCollapse: true,
                    columnDefs: [
                        { targets: 'th', className: 'dt-no-wrap' }
                    ]
                });
            }

            // Event handler for clicking on the table item to get data
            $(document).on('click', '.table_getitemdata', function () {
                var closestTable = $(this).closest('table').DataTable();
                var rowData = closestTable.row($(this).parents('tr')).data();
                CopyFromLine(rowData);
            });

            // Function to handle copying data from the selected line
            function CopyFromLine(rowData) {
                if (rowData && rowData.GdReceiptId) {
                    var idValue = rowData.GdReceiptId;
                    var data = Copyfromheaderdata.find(obj => obj.GdReceiptId === idValue);
                    if (data) {
                        BindHeaderCopyFrom(data);
                    }
                    $.ajax({
                        url: '@Url.Action("GETITEMDET", "GoodsReceipt")',
                        type: 'GET',
                        data: { ID: idValue, Flag: "O" },
                        dataType: 'json',
                        success: function (response) {
                            var i = 1;
                            response.forEach(function (item) {
                                item.BaseDocLineNum = item.LineNum;
                                item.LineNum = i;
                                item.BaseDocEntry = data.DocNum;
                                item.BaseDocType = 'GDR';
                                item.GdIssueDetId = "";
                                item.GdIssueId = "";
                                item.RowStatus = 'O';
                                item.Qty = item.OpenQty;
                                item.Remarks = "";
                            });
                            ApplyDetailstable(response, "Table1");
                            $(".Qty").trigger('input');
                            $('#CopyFromModal').modal('hide');
                        },
                        error: function (error) {
                            notify(error.responseText, "", "top", "right", "fa fa-warning mr-2", "danger", "animated fadeInRight", "animated fadeOutRight");
                            console.error('Error:', error.responseText);
                        }
                    });
                }
            }

            // Function to bind data to header fields from the selected copy data
            function BindHeaderCopyFrom(data) {
                Object.entries(data).forEach(([key, value]) => {
                    var input = $("#" + key);
                    if (!["Tax", "TotBefDis", "TotPayDue", "Status", "DisPer", "DisAmt"].includes(key)) {
                        if (input.hasClass("select2-hidden-accessible")) {
                            input.val(value).trigger("change");
                        } else {
                            if (["DocumentDt", "PostingDt", "RequiredDt"].includes(key)) {
                                value = formatAndSetDate(value);
                            }
                            input.val(value);
                        }
                    }
                });
            }
        }
        // Region Navigation
        {
            var $table = $("#Table");
            $table.find("tbody tr:first").addClass("active");
            $(".btn_forward").click(function () {
                var $currentRow = $table.find(".active");
                var $nextRow = $currentRow.next("tr");
                if ($nextRow.length > 0) {
                    $currentRow.removeClass("active");
                    $nextRow.addClass("active");
                    BindHederRecords($nextRow, "N");
                } else {
                    var $firstRow = $table.find("tbody tr:first");
                    $table.find(".active").removeClass("active");
                    $firstRow.addClass("active");
                    BindHederRecords($firstRow, "N");

                }
            });
            $(".btn_backward").click(function () {
                var $currentRow = $table.find(".active");
                var $prevRow = $currentRow.prev("tr");
                if ($prevRow.length > 0) {
                    $currentRow.removeClass("active");
                    $prevRow.addClass("active");
                    BindHederRecords($prevRow);
                } else {
                    var $lastRow = $table.find("tr:last");
                    $table.find(".active").removeClass("active");
                    $lastRow.addClass("active");
                    BindHederRecords($lastRow, "N");
                }
            });
            $(".btn_first").click(function () {
                var $firstRow = $table.find("tbody tr:first");
                $table.find(".active").removeClass("active");
                $firstRow.addClass("active");
                BindHederRecords($firstRow, "N");
            });
            $(".btn_last").click(function () {
                var $lastRow = $table.find("tr:last");
                $table.find(".active").removeClass("active");
                $lastRow.addClass("active");
                BindHederRecords($lastRow, "N");
            });
            $(".btn_reload").click(function () {
                ResetForm()
                $("#Form_reset_btn").click();
                HideBtn()
            });
        }
    });
</script>

