            // Input change function
            $(document).on("input", ".Tbl_IPrice, .Tbl_Qty", function () {
                var $row = $(this).closest("tr"); // Cache the jQuery object for the current row
                var qty = parseFloat($row.find('.Tbl_Qty input').val()) || 0;
                var price = parseFloat($row.find('.Tbl_IPrice input').val()) || 0;

                // Bind price to Tbl_ItemCost
                $row.find('.Tbl_ItemCost input').val(price);

                if (qty < 1) {
                    $row.find('.Tbl_Qty input').val(1);
                    if (qty < 0) {
                        Swal.fire("<h5>Not Allowed To Enter Negative Values...!</h5>", '', 'warning');
                    } else {
                        Swal.fire("<h5>Quantity Cannot Be Zero...!</h5>", '', 'warning');
                    }
                    Calculation($row);
                    return false;
                }

                var lineTotal = qty * price;
                $row.find('.Tbl_LineTotal input').val(lineTotal.toFixed(2));
                Calculation($row);
            });

            // Calculation function
            function Calculation($row) {
                var totalQty = 0;
                var totalPrice = 0;

                $row.closest('table').find('tbody tr').each(function () {
                    var rowQty = parseFloat($(this).find('.Tbl_Qty input').val()) || 0;
                    var rowPrice = parseFloat($(this).find('.Tbl_IPrice input').val()) || 0;
                    var rowLineTotal = rowQty * rowPrice;

                    totalQty += rowQty;
                    totalPrice += rowLineTotal;

                    $(this).find('.Tbl_LineTotal input').val(rowLineTotal.toFixed(2));
                });

                $('#totalQty').text(totalQty);
                $('#totalPrice').text(totalPrice.toFixed(2));
            }
        }


